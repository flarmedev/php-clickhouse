{
    "name": "flarmedev/php-clickhouse",
    "description": "A modern, fluent, and high-performance ClickHouse driver for PHP.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "clickhouse",
        "database",
        "query-builder",
        "php",
        "laravel"
    ],
    "homepage": "https://php-clickhouse.flarme.com",
    "support": {
        "issues": "https://github.com/flarmedev/php-clickhouse/issues",
        "source": "https://github.com/flarmedev/php-clickhouse"
    },
    "authors": [
        {
            "name": "Flarme",
            "email": "contact@flarme.com",
            "homepage": "https://flarme.com"
        }
    ],
    "require": {
        "php": "^8.4",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "illuminate/support": "^12.43",
        "laravel/pint": "^1.26",
        "pestphp/pest": "^4.1",
        "phpbench/phpbench": "^1.4",
        "phpstan/phpstan": "^2.1",
        "symfony/console": "^7.4"
    },
    "autoload": {
        "psr-4": {
            "Flarme\\PhpClickhouse\\": "src/",
            "Flarme\\PhpClickhouse\\Extra\\Laravel\\": "src/extra/Laravel/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "pest",
        "test:unit": "pest --testsuite=Unit",
        "test:feature": "pest --testsuite=Feature",
        "test:coverage": "pest --coverage --min=80",
        "lint": "pint --test",
        "format": "pint",
        "analyse": "phpstan analyse",
        "benchmark": "phpbench run --report=default"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Flarme\\PhpClickhouse\\Extra\\Laravel\\ClickhouseServiceProvider"
            ]
        }
    },
    "suggest": {
        "illuminate/support": "Required for Laravel integration (^11.0|^12.0)"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
